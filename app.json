{
  "expo": {
    "name": "meuCoracao",
    "slug": "meuCoracao",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/iconeQuadrado.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/iconeSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#a0a4a5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gabrielquadro.meuCoracaoApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconeQuadrado.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.gabrielquadro.meuCoracaoApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "92be2e96-e23e-4c74-ab88-d82f98564dde"
      }
    }
  }
}
